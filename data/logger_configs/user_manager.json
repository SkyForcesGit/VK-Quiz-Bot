{
    "config": {
        "handlers": [
            {
                "sink": "logs/user_manager.log",
                "format": "[TIME: {time:YYYY-MM-DD at HH:mm:ss}] {level} | {message}",
                "level": "DEBUG",
                "serialize": false,
                "compression": "zip",
                "rotation": "1 day"
            }
        ],
        "extra": {"object": "UserManager"}
    }
}